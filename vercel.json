{
  "headers": [
     {
        "source": "/(.*)",
        "headers": [
        {
           "key": "Cross-Origin-Embedder-Policy",
           "value": "credentialless"
        },
        {
           "key": "Cross-Origin-Opener-Policy",
           "value": "same-origin"
        },
        {
           "key": "Cross-Origin-Resource-Policy",
           "value": "cross-origin"
        }
        ]
     },
     {
        "source": "/(.*).(avif|svg)",
        "headers": [
        { 
           "key": "Cache-Control",
           "value": "public,max-age=86400,must-revalidate,stale-while-revalidate=2419200"
        }
        ]
     },
     {
        "source": "/(.*).(woff2|woff|ttf)",
        "headers": [
        { 
           "key": "Cache-Control",
           "value": "public,max-age=604800,must-revalidate,stale-while-revalidate=2419200"
        }
        ]
     }
  ]
}